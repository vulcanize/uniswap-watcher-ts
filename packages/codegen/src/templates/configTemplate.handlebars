[server]
  host = "127.0.0.1"
  port = 3001
  mode = "eth_call"

[upstream]
  [upstream.ethServer]
    gqlApiEndpoint = "http://127.0.0.1:8082/graphql"
    gqlPostgraphileEndpoint = "http://127.0.0.1:5000/graphql"
    rpcProviderEndpoint = "http://127.0.0.1:8081"

  [upstream.cache]
    name = "requests"
    enabled = false
    deleteOnStart = false
